<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<listener>
		<listener-class>com.huilian.spider.servlet.StartupListener</listener-class>
	</listener>
	<servlet>
		<servlet-name>StartupJdTop100SpiderServlet</servlet-name>
		<display-name>StartupJdTop100SpiderServlet</display-name>
		<description></description>
		<servlet-class>com.huilian.spider.servlet.StartupJdTop100SpiderServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>JdTop100SpiderStatusServlet</servlet-name>
		<display-name>JdTop100SpiderStatusServlet</display-name>
		<description></description>
		<servlet-class>com.huilian.spider.servlet.JdTop100SpiderStatusServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>StartupHshSelfGoodsTaskServlet</servlet-name>
		<display-name>StartupHshSelfGoodsTaskServlet</display-name>
		<description></description>
		<servlet-class>com.huilian.spider.servlet.StartupHshSelfGoodsTaskServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StartupJdTop100SpiderServlet</servlet-name>
		<url-pattern>/startupJdTop100Spider</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>JdTop100SpiderStatusServlet</servlet-name>
		<url-pattern>/jdTop100SpiderStatus</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>StartupHshSelfGoodsTaskServlet</servlet-name>
		<url-pattern>/startupHshSelfGoodsTask</url-pattern>
	</servlet-mapping>
</web-app>
